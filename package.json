{
  "name": "Ibori",
  "version": "1.0.1",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client --scheme Ibori",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "npx expo prebuild && eas build --profile development  --platform android",
    "build": "npx expo prebuild && eas build --profile preview  --platform android",
    "prebuild": "npx expo prebuild --platform android",
    "clean": "npx expo prebuild --clean",
    "pod": "cd ios && pod install",
    "adb": "adb devices",
    "update": "npx expo prebuild && eas update --branch preview --platform android",
    "prod": "eas build --profile playstore  --platform android",
    "update-prod": "npx expo prebuild && eas update --branch playstore --platform android",
    "submit": "eas submit --latest --platform android --profile playstore",
    "fix": "npx expo install --fix",
    "dev-ios": "eas build --profile development --platform ios",
    "clear-ios": "npx expo build:ios -c",
    "submit-ios": "eas submit -p ios --latest",
    "build-ios": "eas build --platform ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.1",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^7.3.2",
    "@react-navigation/native": "^7.0.17",
    "@react-navigation/native-stack": "^7.3.1",
    "@react-navigation/stack": "^7.2.1",
    "@reduxjs/toolkit": "^1.6.1",
    "axios": "^1.10.0",
    "expo": "~52.0.40",
    "expo-clipboard": "~7.0.1",
    "expo-dev-client": "~5.0.14",
    "expo-file-system": "~18.0.12",
    "expo-image-manipulator": "~13.0.6",
    "expo-image-picker": "~16.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-local-authentication": "~15.0.2",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "react": "18.3.1",
    "react-native": "0.76.7",
    "react-native-chart-kit": "^6.12.0",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-maps": "1.18.0",
    "react-native-mmkv": "^2.12.2",
    "react-native-pager-view": "^6.8.1",
    "react-native-raw-bottom-sheet": "^2.2.1",
    "react-native-reanimated": "^3.15.3",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-screens": "^4.9.2",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^15.12.0",
    "react-native-swiper": "1.6.0",
    "react-native-tab-view": "^4.1.1",
    "react-native-video": "^6.14.0",
    "react-redux": "^7.2.4",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.12",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^19.0.0",
    "typescript": "^5.8.2"
  },
  "private": true
}
