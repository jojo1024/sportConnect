{
  "expo": {
    "name": "Ibori",
    "slug": "Ibori",
    "version": "1.0.2",
    "scheme": "Ibori",
    "orientation": "portrait",
    "icon": "./assets/images/ibori.jpeg",
    "userInterfaceStyle": "light",
    "primaryColor": "#FFA500",
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "L'application a besoin d'accéder à vos photos pour vous permettre de modifier votre photo de profil.",
          "cameraPermission": "L'application a besoin d'accéder à votre caméra pour vous permettre de prendre une photo de profil."
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/ibori.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "android": {
      "package": "com.joelitho.ibori",
      "foregroundImage": "./assets/images/ibori.jpeg",
      "backgroundColor": "#FFFFFF",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "L'application a besoin d'accéder à votre caméra pour vous permettre de prendre une photo de profil.",
        "NSPhotoLibraryUsageDescription": "L'application a besoin d'accéder à vos photos pour vous permettre de modifier votre photo de profil.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.joelitho.ibori"
    },
    "extra": {
      "projectId": "2e55ba75-f69f-4d16-80e7-0b241fdc5155",
      "eas": {
        "projectId": "2e55ba75-f69f-4d16-80e7-0b241fdc5155"
      }
    }
  }
}
